<h1>Home</h1>
<p id="notice"><%= notice %></p>


<%= form_tag("/log", method: "post") do %>




    <div class="field">
      <%= label_tag(:email, "Email:") %>
      <%= text_field_tag(:email) %>
    </div>
    <div class="field">
      <%= label_tag(:password, "Password:") %>
      <%= text_field_tag(:password) %>
    </div>
    <%= submit_tag("Log in") %>






<% end %>
<%= form_tag("/signup", method: "get") do %>
    <%= submit_tag("Sign up") %>
<% end %>
<p><%=@error%></p>
